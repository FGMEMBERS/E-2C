<?xml version="1.0"?>

<PropertyList>

	<name>light-panel-main-right</name>
	<path>light-panel-main-right.ac</path>


<!-- lighting  -->

	<animation> 
		<object-name>light-panel-main-right</object-name>
		<type>material</type>
		<condition><not><property>sim/rendering/rembrandt/enabled</property></not></condition>
		<emission>
			<factor-prop>controls/lighting/instruments-primary-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>15</max-m>
	</animation>

</PropertyList>
