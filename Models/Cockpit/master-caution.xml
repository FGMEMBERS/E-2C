<?xml version="1.0"?>

<PropertyList>

	<name>master-caution</name>
	<path>master-caution.ac</path>

	<animation>
		<type>select</type>
		<object-name>master-caution-on</object-name>
		<condition>
			<property>sim/model/f-14b/instrumentation/warnings/master-caution</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>master-caution-off</object-name>
		<condition>
			<not>
				<property>sim/model/f-14b/instrumentation/warnings/master-caution</property>
			</not>
		</condition>
	</animation>

<!-- lighting  -->

	<animation> 
		<object-name>panel</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/instruments-primary-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>

	<animation>
		<object-name>master-caution-on</object-name>
		<type>material</type>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>



	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>15</max-m>
	</animation>

</PropertyList>
