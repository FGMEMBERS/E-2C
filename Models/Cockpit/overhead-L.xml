<?xml version="1.0"?>

<PropertyList>

	<name>overhead-L</name>
	<path>overhead-L.ac</path>



	<!--fuel -->
	<animation>
		<type>select</type>
		<object-name>tank-interconnect-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/fuel/tank-interconnect</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>tank-interconnect-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/fuel/tank-interconnect</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>fuel-dump-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/fuel/fuel-dump</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>fuel-dump-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/fuel/fuel-dump</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>hotspot-fuel-dump-up</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
					<property>sim/model/E-2C/controls/fuel/fuel-dump</property>
					<value>true</value>
			</binding>
		</action>
	</animation> 
	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>hotspot-fuel-dump-down</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
					<property>sim/model/E-2C/controls/fuel/fuel-dump</property>
					<value>false</value>
			</binding>
		</action>
	</animation> 
	<animation>
		<type>rotate</type>
		<object-name>fuel-dump-sw</object-name>
		<property>sim/model/E-2C/controls/fuel/fuel-dump</property>
		<factor>60</factor>
		<center>
			<x-m>0.0044</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.0447</z-m>
		</center>
		<axis><y>-1</y></axis>
	</animation>


	<!--props -->
	<animation>
		<type>select</type>
		<object-name>NTS-L-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/propellers/propeller[0]/NTS-running</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>NTS-L-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/propellers/propeller[0]/NTS-running</property>
			</not>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>NTS-R-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/propellers/propeller[1]/NTS-running</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>NTS-R-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/propellers/propeller[1]/NTS-running</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>sync-master-stby-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/propellers/sync-master-stby</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>sync-master-stby-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/propellers/sync-master-stby</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>auto-feather-arm-on</object-name>
		<condition>
			<property>sim/model/E-2C/controls/propellers/auto-feather-arm</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>auto-feather-arm-off</object-name>
		<condition>
			<not>
				<property>sim/model/E-2C/controls/propellers/auto-feather-arm</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>fuel-gov-check-on</object-name>
		<condition>
			<not>
				<equals>
					<property>sim/model/E-2C/controls/propellers/fuel-gov-check</property>
					<value>0</value>
				</equals>
			</not>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>fuel-gov-check-off</object-name>
		<condition>
			<equals>
				<property>sim/model/E-2C/controls/propellers/fuel-gov-check</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>pump-L-up-on</object-name>
		<object-name>pump-L-down-on</object-name>
		<condition>
			<less-than-equals>
				<property>controls/engines/engine[0]/throttle</property>
				<value>0</value>
			</less-than-equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>pump-R-up-on</object-name>
		<object-name>pump-R-down-on</object-name>
		<condition>
			<less-than-equals>
				<property>controls/engines/engine[1]/throttle</property>
				<value>0</value>
			</less-than-equals>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>pump-L-up-off</object-name>
		<object-name>pump-L-down-off</object-name>
		<condition>
			<greater-than>
				<property>controls/engines/engine[0]/throttle</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>pump-R-up-off</object-name>
		<object-name>pump-R-down-off</object-name>
		<condition>
			<greater-than>
				<property>controls/engines/engine[1]/throttle</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>



	<!--engines -->
	<animation>
		<type>select</type>
		<object-name>dec-L-on</object-name>
		<condition>
			<equals>
				<property>sim/model/E-2C/controls/engines/engine[0]/DEC-control</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>dec-R-on</object-name>
		<condition>
			<equals>
				<property>sim/model/E-2C/controls/engines/engine[1]/DEC-control</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>dec-L-off</object-name>
		<condition>
			<greater-than>
				<property>sim/model/E-2C/controls/engines/engine[0]/DEC-control</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>dec-R-off</object-name>
		<condition>
			<greater-than>
				<property>sim/model/E-2C/controls/engines/engine[1]/DEC-control</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>eng-start-L-on</object-name>
		<condition>
			<greater-than>
				<property>sim/model/E-2C/controls/engines/engine[0]/start-seq</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>eng-start-R-on</object-name>
		<condition>
			<greater-than>
				<property>sim/model/E-2C/controls/engines/engine[1]/start-seq</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>eng-start-L-off</object-name>
		<condition>
			<equals>
				<property>sim/model/E-2C/controls/engines/engine[0]/start-seq</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>eng-start-R-off</object-name>
		<condition>
			<equals>
				<property>sim/model/E-2C/controls/engines/engine[1]/start-seq</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>








	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>50</max-m>
	</animation>

</PropertyList>
