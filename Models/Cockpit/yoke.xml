<?xml version="1.0"?>

<PropertyList>

	<name>yoke</name>
	<path>yoke.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>column-assembly</object-name>
		<property>controls/flight/elevator</property>
		<center><z-m>-0.5534</z-m></center>
		<axis><y>1</y></axis>
		<interpolation>
			<entry><ind> -1</ind><dep>8.11</dep></entry>
			<entry><ind>  0</ind><dep>0</dep></entry>
			<entry><ind>  1</ind><dep>-14.6</dep></entry>
		</interpolation>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>belows-center</object-name>
		<property>controls/flight/elevator</property>
		<center><z-m>-0.5534</z-m></center>
		<axis><y>1</y></axis>
		<interpolation>
			<entry><ind> -1</ind><dep>7</dep></entry>
			<entry><ind>  0</ind><dep>0</dep></entry>
			<entry><ind>  1</ind><dep>-10</dep></entry>
		</interpolation>
	</animation>


	<animation>
		<type>rotate</type>
		<object-name>wheel</object-name>
		<property>controls/flight/aileron</property>
		<factor>-52.5</factor>
		<axis>
			<x1-m>-0.2332</x1-m><y1-m>0</y1-m><z1-m>-0.0026</z1-m>
			<x2-m>0.1903</x2-m><y2-m>0</y2-m><z2-m>0.0998</z2-m>
		</axis>
	</animation>
	




