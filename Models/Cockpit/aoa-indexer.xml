	<!-- lighting -->
	<animation>
		<object-name>aoa-too-slow</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>sim/model/E-2C/controls/lighting/index-norm</factor-prop>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
		</emission>
	</animation>
	<animation>
		<object-name>aoa-slow</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>sim/model/E-2C/controls/lighting/index-norm</factor-prop>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
		</emission>
	</animation>
	<animation>
		<object-name>aoa-on-speed</object-name>
