<?xml version="1.0"?>

<PropertyList>

	<path>aoa-indexers.ac</path>

	<animation>
		<type>select</type>
		<object-name>aoa-too-slow</object-name>
		<condition>
			<!--<greater-than>
				<property>systems/electrical/outputs/aoa-indexer</property>
				<value>23</value>
			</greater-than>-->
			<greater-than>
				<property>orientation/alpha-deg</property>
				<property>sim/model/E-2C/instrumentation/aoa-indexer/too-slow-deg</property>
			</greater-than>
			<equals>
				<property>gear/gear[0]/position-norm</property>
				<value>1</value>
			</equals>
			<or>
				<!--<property>sim/model/E-2C/controls/lighting/hook-bypass</property>-->
				<and>
					<less-than>
						<property>gear/tailhook/position-norm</property>
						<value>1</value>
					</less-than>
					<property>sim/model/E-2C/lighting/warn-medium-lights-switch/state</property>
				</and>
				<equals>
					<property>gear/tailhook/position-norm</property>
					<value>1</value>
				</equals>
			</or>

		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>aoa-slow</object-name>
		<condition>
			<!--<greater-than>
				<property>systems/electrical/outputs/aoa-indexer</property>
